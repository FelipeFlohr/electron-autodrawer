{
  "build": {
    "appId": "com.felipeflohr",
    "extends": null,
    "files": [
      "./package.json",
      "./dist/**/*.js",
      "./dist/**/*.png",
      "./dist/**/*.css",
      "./dist/**/*.html"
    ],
    "icon": "./res/icon.ico",
    "win": {
      "target": "portable"
    }
  },
  "postinstall": "electron-builder install-app-deps",
  "name": "autodrawer",
  "version": "0.9.0",
  "description": "Autodrawer made using Electron",
  "main": "dist/index.js",
  "scripts": {
    "build": "gulp",
    "build-electron": "gulp && cd dist && cross-env NODE_ENV=production electron index.js",
    "build-electron-dev": "gulp && cd dist && cross-env NODE_ENV=dev electron index.js",
    "build-dist": "gulp && cross-env NODE_ENV=production electron-builder",
    "electron": "cd dist && electron index.js",
    "electron-dev": "cd dist && cross-env NODE_ENV=dev electron index.js",
    "dist": "electron-builder",
    "rebuild": "npm rebuild --runtime=electron --target=13.6.9 --disturl=https://atom.io/download/atom-shell --abi=79"
  },
  "keywords": [
    "autodrawer",
    "electron"
  ],
  "author": "Felipe Matheus Flohr",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@types/node": "^17.0.21",
    "bootstrap": "^5.1.3",
    "cross-env": "^7.0.3",
    "electron": "13.6.9",
    "electron-builder": "^22.14.13",
    "electron-rebuild": "^3.2.7",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-sass": "^5.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify-es": "^3.0.0",
    "gulp-uglifycss": "^1.1.0",
    "merge-stream": "^2.0.0",
    "node-loader": "^2.0.0",
    "sass": "^1.49.9",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-stream": "^7.0.0"
  },
  "dependencies": {
    "jimp": "^0.16.1",
    "robotjs": "^0.6.0"
  }
}